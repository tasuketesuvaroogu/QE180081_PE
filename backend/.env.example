# Database Settings
DatabaseSettings__ConnectionString=mongodb://localhost:27017
DatabaseSettings__DatabaseName=MovieWatchlistDb
DatabaseSettings__MoviesCollectionName=Movies
DatabaseSettings__UsersCollectionName=Users

# JWT Settings
JwtSettings__Issuer=PRN.MovieWatchlist
JwtSettings__Audience=PRN.MovieWatchlist.Client
JwtSettings__SecretKey=your-secret-key-here-change-in-production
JwtSettings__AccessTokenExpirationMinutes=120

# Upload Settings
UploadSettings__ExternalUploadUrl=https://static.daringtact.live/api/upload
UploadSettings__ExternalUploadToken=Bearer your-upload-token-here

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft.AspNetCore=Warning

# ASP.NET Core
ASPNETCORE_URLS=http://+:8080
ASPNETCORE_ENVIRONMENT=Development
